<queue-entries>
  <queue>
    <name>FetchFeedQueue</name>
    <rate>1/s</rate>
    <retry-parameters>
      <task-retry-limit>3</task-retry-limit>
      <task-age-limit>3m</task-age-limit>
    </retry-parameters>
  </queue>
  <queue>
    <name>FetchPageQueue</name>
    <rate>1/s</rate>
    <retry-parameters>
      <task-retry-limit>3</task-retry-limit>
      <task-age-limit>2m</task-age-limit>
    </retry-parameters>
  </queue>
  <queue>
    <name>WashPageQueue</name>
    <rate>5/s</rate>
    <retry-parameters>
      <task-retry-limit>2</task-retry-limit>
      <task-age-limit>2m</task-age-limit>
    </retry-parameters>
  </queue>
  <queue>
    <name>PostQueue</name>
    <rate>5/s</rate>
    <retry-parameters>
      <task-retry-limit>3</task-retry-limit>
      <task-age-limit>3m</task-age-limit>
    </retry-parameters>
  </queue>
</queue-entries>